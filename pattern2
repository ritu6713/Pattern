public class diamond {
    public static void main(String[] args) {
        int n=5;

    for(int i=0;i<=n;i++){
      //innerloop 
        for(int j=1;j<=n-1;j++){
        System.out.print(" ");
       }
       for(int j=1;j<=2*i-1;j++){
        System.out.print("*");
       }
       System.out.println();
    }
    }
    
}
